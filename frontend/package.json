		"@tailwindcss/typography": "^0.5.16",
		"@vitest/browser": "^3.1.1",
		"@vitest/coverage-v8": "^3.1.1",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.19",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.19",
		"drizzle-kit": "^0.30.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
